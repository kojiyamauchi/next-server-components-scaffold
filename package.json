{
  "name": "next-server-components",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@4.6.0",
  "engines": {
    "npm": "use yarn instead.",
    "yarn": "4.6.0"
  },
  "prisma": {
    "seed": "yarn tsx prisma/seeds/index.ts"
  },
  "scripts": {
    "prepare": "husky install",
    "postinstall": "prisma generate",
    "dev:path": "pathpida -o ./src/libs -w",
    "dev:app": "next dev --turbopack",
    "dev": "run-p \"dev:*\"",
    "storybook": "NEXT_PUBLIC_STORYBOOK=enable BROWSER='Google Chrome Canary' storybook dev -p 6006",
    "build:path": "pathpida -o ./src/libs/path/",
    "build:storybook": "storybook build",
    "build:app": "next build --turbopack",
    "build": "run-p \"build:*\"",
    "start": "next start",
    "lint": "eslint --fix --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "jest": "jest --passWithNoTests",
    "spellcheck": "cspell --dot --no-must-find-files '**/*'",
    "ui": "yarn upgrade-interactive --latest",
    "db:start": "npx supabase@latest start",
    "db:stop": "npx supabase@latest stop",
    "db:reset": "npx supabase@latest db reset",
    "prisma:init": "prisma init",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:reset": "prisma migrate reset",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:status": "prisma migrate status",
    "prisma:push": "prisma db push",
    "prisma:pull": "prisma db pull",
    "prisma:seed": "prisma db seed",
    "prisma:validate": "prisma validate",
    "prisma:format": "prisma format",
    "prisma:studio": "prisma studio -b 'Google Chrome Canary'"
  },
  "dependencies": {
    "@types/jest": "30.0.0",
    "eslint-plugin-jest": "29.0.1",
    "jest": "30.1.1",
    "jest-environment-jsdom": "30.1.1",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "ts-jest": "29.4.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "10.0.0",
    "@headlessui/react": "2.2.7",
    "@prisma/client": "6.5.0",
    "@storybook/addon-a11y": "^9.1.3",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-onboarding": "^9.1.3",
    "@storybook/addon-vitest": "^9.1.3",
    "@storybook/nextjs-vite": "^9.1.3",
    "@t3-oss/env-nextjs": "0.13.8",
    "@tailwindcss/postcss": "^4",
    "@types/eslint": "9.6.1",
    "@types/identity-obj-proxy": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "8.41.0",
    "@typescript-eslint/parser": "8.41.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "cspell": "9.2.0",
    "dayjs": "1.11.18",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-storybook": "^9.1.3",
    "globals": "16.3.0",
    "husky": "9.1.7",
    "identity-obj-proxy": "3.0.0",
    "lint-staged": "16.1.5",
    "npm-run-all": "4.1.5",
    "pathpida": "0.25.0",
    "playwright": "^1.55.0",
    "prettier": "3.6.2",
    "prisma": "6.5.0",
    "react-loading-skeleton": "3.5.0",
    "storybook": "^9.1.3",
    "tailwindcss": "^4",
    "tsx": "4.20.5",
    "typescript": "^5",
    "typescript-eslint": "8.41.0",
    "vitest": "^3.2.4",
    "zod": "4.1.5"
  }
}
